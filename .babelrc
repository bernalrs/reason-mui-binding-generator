{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "esmodules": true
      }
    }],
    ["@babel/preset-stage-1", {"decoratorsLegacy": true}],
    "@babel/preset-react",
    "next/babel",
    "@babel/preset-es2015"
  ],
  "plugins": [
    "preval",
    "@babel/plugin-transform-object-assign",
    "flow-react-proptypes",
    "@babel/plugin-transform-flow-strip-types"
  ]
}
